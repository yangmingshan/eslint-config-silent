{
  "name": "eslint-config-silent",
  "version": "0.17.0",
  "description": "Silence all rules that you don't have to care about.",
  "main": "index.js",
  "repository": "https://github.com/yangmingshan/eslint-config-silent.git",
  "author": "Yang Mingshan <y.mingshan3@gmail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "cross-env NODE_ENV=production eslint .",
    "test": "ava",
    "prepublishOnly": "yarn lint && yarn test"
  },
  "files": [
    "@typescript-eslint.js",
    "ava.js",
    "babel.js",
    "flowtype.js",
    "import.js",
    "index.js",
    "prettier.js",
    "react.js",
    "unicorn.js",
    "vue.js"
  ],
  "keywords": [
    "eslint",
    "eslintconfig",
    "silent",
    "silence",
    "fix",
    "fixable",
    "quiet",
    "peaceful",
    "ignore"
  ],
  "devDependencies": {
    "ava": "^3.1.0",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.0",
    "eslint": "^7.4.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-vue": "^6.0.1",
    "husky": "^4.2.1",
    "is-plain-obj": "^2.0.0",
    "lint-staged": "^10.0.3",
    "typescript": "^4.0.3",
    "xo": "^0.33.1"
  },
  "peerDependencies": {
    "eslint": ">=6.5.1"
  },
  "resolutions": {
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "eslint-plugin-ava": "^11.0.0",
    "eslint-plugin-unicorn": "^22.0.0",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "ava"
    }
  },
  "lint-staged": {
    "*.js": "cross-env NODE_ENV=production eslint --fix",
    "*.json": "prettier --write"
  }
}
