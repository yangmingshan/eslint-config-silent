{
  "name": "eslint-config-silent",
  "version": "0.2.0",
  "description": "Silence all rules that you don't have to care about.",
  "main": "index.js",
  "repository": "https://github.com/yangmingshan/eslint-config-silent.git",
  "author": "Yang Mingshan <y.mingshan3@gmail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "cross-env NODE_ENV=production eslint .",
    "test": "ava",
    "prepublish": "yarn lint && yarn test"
  },
  "files": [
    "@typescript-eslint.js",
    "ava.js",
    "babel.js",
    "flowtype.js",
    "import.js",
    "index.js",
    "prettier.js",
    "react.js",
    "unicorn.js",
    "vue.js"
  ],
  "keywords": [
    "eslint",
    "eslintconfig",
    "silent",
    "silence",
    "fix",
    "fixable",
    "quiet",
    "peaceful",
    "ignore"
  ],
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@typescript-eslint/eslint-plugin": "^2.4.0",
    "@typescript-eslint/parser": "^2.4.0",
    "ava": "^2.4.0",
    "babel-eslint": "^10.0.3",
    "cross-env": "^6.0.3",
    "eslint": "^6.5.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-vue": "^6.0.1",
    "husky": "^3.0.9",
    "is-plain-obj": "^2.0.0",
    "lint-staged": "^9.4.2",
    "temp-write": "^4.0.0",
    "typescript": "^3.6.4",
    "xo": "^0.25.3"
  },
  "peerDependencies": {
    "eslint": ">=6.5.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "ava"
    }
  },
  "lint-staged": {
    "*.js": [
      "cross-env NODE_ENV=production eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  }
}
